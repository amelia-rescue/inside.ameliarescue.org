{
  "name": "inside.ameliarescue.org",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "dev:debug": "react-router dev --profile",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "react-router-serve ./build/server/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "react-router typegen && tsc",
    "cdk": "cdk",
    "deploy:dns": "npm run cdk deploy InsideAmeliaRescueDnsStack -- --profile aes",
    "deploy:app": "npm run build && npm run cdk deploy InsideAmeliaRescueStack -- --profile aes",
    "destroy": "npm run cdk destroy -- --profile aes",
    "diff": "npm run cdk diff -- --profile aes"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.965.0",
    "@aws-sdk/lib-dynamodb": "^3.965.0",
    "@react-router/architect": "^7.12.0",
    "@react-router/fs-routes": "^7.12.0",
    "@react-router/node": "^7.12.0",
    "@react-router/serve": "^7.12.0",
    "isbot": "^5.1.31",
    "oauth4webapi": "^3.8.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.12.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.12.0",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "aws-cdk": "^2.1100.1",
    "aws-cdk-lib": "^2.233.0",
    "constructs": "^10.4.2",
    "daisyui": "^5.5.14",
    "dynalite": "^4.0.0",
    "happy-dom": "^20.0.5",
    "jsdom": "^27.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
