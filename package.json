{
  "name": "inside.ameliarescue.org",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "dev:debug": "react-router dev --profile",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "cdk": "cdk",
    "deploy": "npm run build && npm run cdk deploy -- --profile aes",
    "destroy": "npm run cdk destroy -- --profile aes",
    "diff": "npm run cdk diff -- --profile aes"
  },
  "dependencies": {
    "@react-router/architect": "^7.9.4",
    "@react-router/node": "^7.9.2",
    "@react-router/serve": "^7.9.2",
    "isbot": "^5.1.31",
    "oauth4webapi": "^3.8.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.2"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.2",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "aws-cdk": "^2.1030.0",
    "aws-cdk-lib": "^2.220.0",
    "constructs": "^10.4.2",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
